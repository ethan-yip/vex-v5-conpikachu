f1.spin(FORWARD)\n        f2.spin(FORWARD)\n    if wt = \"rw\":\n        print(\"Rear Wheel Drive\")\n        # Rear Wheel Drive\n        r1.spin(FORWARD)\n        r2.spin(FORWARD)\n\n\nif __name__ == \"__main__\":\n    # Initiation Function\n    while init:\n        if brain.screen.pressing() == True:\n            init = False\n    ","textLanguage":"python","robotConfig":[{"port":[1],"name":"r1","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"false","fwd":"forward","rev":"reverse","gear":"ratio18_1"}},{"port":[2],"name":"r2","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"false","fwd":"forward","rev":"reverse","gear":"ratio18_1"}},{"port":[3],"name":"f1","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"false","fwd":"forward","rev":"reverse","gear":"ratio18_1"}},{"port":[4],"name":"f2","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"false","fwd":"forward","rev":"reverse","gear":"ratio18_1"}}],"slot":0,"platform":"V5","sdkVersion":"20240802.15.00.00","appVersion":"4.0.7","minVersion":"3.1.0","fileFormat":"2.0.0","targetBrainGen":"First","v5Sounds":[{"name":"game over","url":"static/sounds/mixkit-arcade-retro-game-over-213.wav"}],"v5SoundsEnabled":false,"target":"Physical"}